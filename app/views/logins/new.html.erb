    
  <div class="Login">
    
    <%= form_tag login_path do %>

    	<div class="field">
    		<%= label_tag :email %><br />
        	<%= text_field_tag :email, params[:email] %>
      	</div>

      	<div class="field">
        	<%= label_tag :password %><br />
        	<%= password_field_tag :password %>
      	</div>

      	<%= submit_tag "Log In" %>
      </div>
    <% end %>
    
  </div>